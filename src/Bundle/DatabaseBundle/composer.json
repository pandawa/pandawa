{
    "name": "pandawa/database-bundle",
    "description": "The pandawa database bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Iqbal Maulana",
            "email": "iq.bluejack@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/database": "^9.0",
        "pandawa/foundation-bundle": "^5.0",
        "pandawa/dependency-injection-bundle": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Pandawa\\Bundle\\DatabaseBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.x-dev"
        },
        "laravel": {
            "providers": [
                "Pandawa\\Bundle\\DatabaseBundle\\DatabaseBundle"
            ],
            "aliases": {
                "DB": "Illuminate\\Support\\Facades\\DB"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
