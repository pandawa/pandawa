{
    "name": "pandawa/cache-bundle",
    "description": "The pandawa cache bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Iqbal Maulana",
            "email": "iq.bluejack@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/contracts": "^9.0|^10.0",
        "illuminate/support": "^9.0|^10.0",
        "illuminate/cache": "^9.0|^10.0",
        "pandawa/foundation-bundle": "^5.0",
        "pandawa/dependency-injection-bundle": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Pandawa\\Bundle\\CacheBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.x-dev"
        },
        "laravel": {
            "providers": [
                "Pandawa\\Bundle\\CacheBundle\\CacheBundle"
            ],
            "aliases": {
                "Cache": "Illuminate\\Support\\Facades\\Cache"
            }
        }
    },
    "suggest": {
        "pandawa/redis-bundle": "Install this package to use redis as cache store",
        "pandawa/database-bundle": "Install this package to use database as cache store"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
