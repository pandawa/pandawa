        }
    ],
    "require": {
        "php": ">=8.1",
        "laravel/helpers": "^1.0",
        "pandawa/foundation": "^5.0",
        "symfony/config": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Pandawa\\Bundle\\FoundationBundle\\": ""
        }
