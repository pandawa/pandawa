{
    "name": "pandawa/validation-bundle",
    "description": "The pandawa validation bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Iqbal Maulana",
            "email": "iq.bluejack@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "pandawa/dependency-injection-bundle": "^5.0",
        "pandawa/foundation-bundle": "^5.0",
        "pandawa/translation-bundle": "^5.0",
        "pandawa/validation": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Pandawa\\Bundle\\ValidationBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.x-dev"
        },
        "laravel": {
            "providers": [
                "Pandawa\\Bundle\\ValidationBundle\\ValidationBundle"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
