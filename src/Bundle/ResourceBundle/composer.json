{
    "name": "pandawa/resource-bundle",
    "description": "The pandawa api resource bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Iqbal Maulana",
            "email": "iq.bluejack@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "pandawa/dependency-injection-bundle": "^5.0",
        "pandawa/foundation-bundle": "^5.0",
        "pandawa/routing-bundle": "^5.0",
        "pandawa/validation-bundle": "^5.0",
        "pandawa/serializer-bundle": "^5.0",
        "pandawa/translation-bundle": "^5.0",
        "pandawa/resource": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Pandawa\\Bundle\\ResourceBundle\\": ""
        }
    },
    "suggest": {
        "pandawa/eloquent-bundle": "Install this bundle to enable api with type resource",
        "pandawa/bus-bundle": "Install this bundle to enable api with type message"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.x-dev"
        },
        "laravel": {
            "providers": [
                "Pandawa\\Bundle\\ResourceBundle\\ResourceBundle"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
