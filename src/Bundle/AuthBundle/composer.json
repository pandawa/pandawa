{
    "name": "pandawa/auth-bundle",
    "description": "The pandawa auth bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Iqbal Maulana",
            "email": "iq.bluejack@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "pandawa/dependency-injection-bundle": "^5.0",
        "pandawa/foundation-bundle": "^5.0",
        "illuminate/auth": "^9.0|^10.0"
    },
    "suggest": {
        "pandawa/annotation-bundle": "Install to enable annotation feature.",
        "pandawa/auth-annotations": "Auth annotations"
    },
    "autoload": {
        "psr-4": {
            "Pandawa\\Bundle\\AuthBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.x-dev"
        },
        "laravel": {
            "providers": [
                "Pandawa\\Bundle\\AuthBundle\\AuthBundle"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
