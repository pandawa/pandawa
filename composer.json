{
    "name": "pandawa/pandawa",
    "description": "Modular Laravel Framework",
    "keywords": [
        "laravel",
        "pandawa"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Iqbal Maulana",
            "email": "iq.bluejack@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "doctrine/annotations": "^1.13",
        "laravel/framework": "^9.0",
        "laravel/helpers": "^1.0",
        "spiral/attributes": "^3.0",
        "spiral/tokenizer": "^2.13",
        "symfony/config": "^6.0",
        "symfony/yaml": "^6.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Pandawa\\Component\\": "src/Component/",
            "Pandawa\\Bundle\\": "src/Bundle/",
            "Pandawa\\Annotations\\": "src/Annotations/",
            "Pandawa\\Contracts\\": "src/Contracts/"
        },
        "files": [
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.x-dev"
        },
        "laravel": {
            "providers": [
            ]
        }
    },
    "prefer-stable": true
}
